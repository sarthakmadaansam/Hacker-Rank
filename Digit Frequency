#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i,j,k,c0,c1,c2,c3,c4,c5,c6,c7,c8,c9,a[10],h,x;
    char s[1000];
    fgets(s,1000,stdin);
    k=strlen(s);
    for(i=0;i<10;i++)
    {
        a[i]=0;
    }
    for(i=0;i<k;i++)
    {
        if(s[i]=='0')
        {
            a[0]=1+a[0];
        }
        else if(s[i]=='1')
        {
            a[1]=a[1]+1;
        }
        else if(s[i]=='2')
        {
            a[2]=a[2]+1;
        }
        else if(s[i]=='3')
        {
            a[3]=a[3]+1;
        }
        else if(s[i]=='4')
        {
            a[4]=a[4]+1;
        }
        else if(s[i]=='5')
        {
            a[5]=a[5]+1;
        }
        else if(s[i]=='6')
        {
            a[6]=a[6]+1;
        }
        else if(s[i]=='7')
        {
            a[7]=a[7]+1;
        }
        else if(s[i]=='8')
        {
            a[8]=a[8]+1;
        }
        else if(s[i]=='9')
        {
            a[9]=a[9]+1;
        }
        else {
        h=x;
        }
    }
    for(i=0;i<10;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
